<c74object name="mvc.list.thindex" module="mvc" kind="patcher" category="library">

	<digest>
		Report list of indices of first appearance of an item in a list.
	</digest>

	<description>
		mvc.list.thindex both thins an incoming list and reports the indices of first appearance of an item in a list.
	</description>
	
	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Vincent Goudard</metadata>
		<metadata name="tag">MVC</metadata>
		<metadata name="tag">mapping</metadata>
		<metadata name="tag">library</metadata>
	</metadatalist>
	
	<!--INLETS-->
	<inletlist>
		<inlet id="0" name="list input" type="list">
			<digest>List to be processed.</digest>
		</inlet>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
		<inlet id="0" name="message output" type="any">
			<digest>Indices of first items appearance</digest>
		</inlet>
		<inlet id="0" name="message output" type="any">
			<digest>Thinned list</digest>
		</inlet>
	</outletlist>
	
	<!--ARGUMENTS-->
	<objarglist>

	</objarglist>
	
	<!--MESSAGES-->
